{
  "name": "akl-mylab",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --host",
    "publishing": "vite --mode publishing",
    "build": "vue-tsc --noEmit && vite build",
    "build:dev": "vue-tsc --noEmit && vite build --mode=staging",
    "preview": "vite preview",
    "lint": "npx eslint --ignore-path .prettierignore --ext .ts,.js,.vue .",
    "lint-fix": "npx eslint --ignore-path .prettierignore --ext .ts,.js,.vue --fix .",
    "lint:ls": "ls-lint"
  },
  "dependencies": {
    "@brightcove/player-loader": "^1.8.0",
    "@vuepic/vue-datepicker": "^3.2.0",
    "@vueuse/core": "^8.4.2",
    "@vueuse/shared": "^8.4.2",
    "axios": "^0.27.2",
    "dayjs": "^1.11.2",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.14",
    "qs": "^6.10.3",
    "swiper": "^8.1.4",
    "vue": "^3.2.33",
    "vue-final-modal": "^3.4.3",
    "vue-router": "4.0.15"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.31",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "lint-staged": "^12.4.1",
    "ls-lint": "^0.1.2",
    "msw": "^0.39.2",
    "prettier": "^2.6.2",
    "sass": "^1.51.0",
    "typescript": "^4.6.2",
    "unplugin-icons": "^0.14.3",
    "unplugin-vue-components": "^0.19.5",
    "vite": "^2.9.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pages": "^0.23.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-layouts": "^0.6.0",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-tsc": "^0.34.13"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
